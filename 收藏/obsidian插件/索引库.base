views:
  - type: table
    name: 表格
    filters:
      and:
        - file.folder.startsWith("收藏/obsidian插件")
        - '!file.basename.contains("索引")'
    order:
      - file.name
      - 简介
      - 分类
    sort:
      - property: 分类
        direction: DESC
      - property: file.ctime
        direction: DESC
    columnSize:
      note.简介: 183
